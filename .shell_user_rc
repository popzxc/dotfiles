# Preinstalled Rocksdb & snappy directories
export ROCKSDB_LIB_DIR=/usr/lib
export SNAPPY_LIB_DIR=/usr/lib

# Setup `sccache` as a `rustc` wrapper.
export RUSTC_WRAPPER=sccache
export SCCACHE_DIR=#SET TO CORRECT VALUE
export SCCACHE_CACHE_SIZE="50g"

# Override default directory for stack.
export STACK_ROOT=#SET TO CORRECT VALUE
